#!/bin/sh -e

libdir="/Users/jayyang/work/unity-curl/build/nghttp2/nghttp2-1.36.0/lib"
if [ -d "$libdir/.libs" ]; then
    libdir="$libdir/.libs"
fi

export CGO_CFLAGS="-I/Users/jayyang/work/unity-curl/build/nghttp2/nghttp2-1.36.0/lib/includes -I/Users/jayyang/work/unity-curl/build/nghttp2/nghttp2-1.36.0/lib/includes"
export CGO_LDFLAGS="-L$libdir"
export LD_LIBRARY_PATH="$libdir"
export GODEBUG=cgocheck=0
"$@"
